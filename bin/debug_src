  function print_dict
  {
    local -n ref="${1}"

    echo "'${1}'"

    for key in ${!ref[@]}; do
      local value="${ref[$key]}"
      echo -e "'${key}' '${value}'"
    done

    echo
  }

  function print_list
  {
    local -n ref="${1}"

    echo "'${1}'"

    for value in ${ref[@]}; do
      echo -e "'${value}'"
    done

    echo
  }

  function print_var
  {
    local -n ref="${1}"
    echo -e "'${1}' '${ref}'\n"
  }